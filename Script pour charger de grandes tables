SET UNIQUE_CHECKS=0;
SET FOREIGN_KEY_CHECKS=0;

truncate TABLE `name_basics`;
LOAD DATA LOCAL INFILE 'name.basics.tsv'
INTO TABLE `name_basics`
FIELDS TERMINATED BY '\t' ENCLOSED BY ''
LINES TERMINATED BY '\n'
IGNORE 1 LINES;

